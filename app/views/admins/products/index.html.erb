<%= render 'admins/users/header'  %>

<h1>注文一覧</h1>
<%= button_to '新規追加', admins_new_product_path, class:"btn btn-default" %>
<div class="container">
	<div class="col-6">
    	<table class="table">
      		<thead>
        		<tr>
            		<th>画像</th>
            		<th>シングル/アルバム名</th>
            		<th>アーティスト名</th>
            		<th>値段</th>
            		<th>在庫数</th>
            		<th>お気に入り数</th>
        		</tr>
      		</thead>
      		<tbody>
        		<tr>
           		 <% @products.each do |product| %>
<!--            		 	attachment_image_tag -->
           		 	<td><%= product.album_title %></td>
            		<td><%= product.artist %></td>
            		<td><%= product.price %></td>
            		<td><%= product.stock %></td>
            		<td><%= product.favorite_count %></td>
            	<% end %>
        		</tr>
      		</tbody>
    	</table>
    	<%= paginate @products %>
  	</div>
</div>
